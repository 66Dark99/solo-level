{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "no-cache"
            }
        }
    ],
    "functions": {
        "server.js": {
            "memory": 128,
            "maxDuration": 10
        }
    }
}